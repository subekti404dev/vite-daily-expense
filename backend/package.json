{
  "name": "daily-expense-be",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:js && npm run build:cjs && npm run build:bin",
    "build:clean": "rimraf dist/",
    "build:js": "npx tsc",
    "build:cjs": "npx esbuild dist/index.js  --bundle --outfile=dist/app.cjs --format=cjs --platform=node",
    "build:bin": "ncc build dist/app.cjs -o dist && rimraf dist/middlewares dist/routes dist/utils dist/app.cjs dist/index.js && pkg dist/index.cjs -o app",
    "start": "node dist/index.js",
    "dev": "pnpm build && concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@vercel/ncc": "^0.38.1",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "pocketbase": "^0.19.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.201",
    "@types/md5": "^2.3.5",
    "@types/node": "^18.11.9",
    "concurrently": "^7.5.0",
    "nodemon": "^2.0.20",
    "pkg": "^5.8.1",
    "rimraf": "^5.0.5",
    "typescript": "^4.8.4"
  }
}
